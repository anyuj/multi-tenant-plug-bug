    "@payloadcms/richtext-lexical": "^3.35.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.11.0",
    "next": "15.3.1",
    "payload": "^3.35.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sharp": "0.32.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^22.15.3",
    "@types/react": "19.0.10",
